<!doctype html>
<html lang="en">




<body>
    <!--loading -->
    <div class="loader">
        <div class="loader-element"></div>
    </div>

                           <li class="nav-item">
                                    <a class="nav-link" href="contact.html"> contact </a>
                           </li>
                          
                       


    <!-- blog-home9-->
    <div class="blog-home9">

        <div class="container-wrap">
       
        </div>

        <!--post 2-->

        <div class="row mt-5">
            <div class="col-12">
                <h2>Kategoriler</h2>
                <hr />
            </div>
            <div class="col-12">
                <div class="row">
                    @foreach (var kategori in Model.Category)
                    {
                        <div class="col-3">
                            <div class="card">
                                <img src="~/Content/Admin/assets/images/category/@kategori.Image" class="card-img-top" />

                                <div class="card-body">
                                    <h5>@kategori.Name</h5>
                                    @if (kategori.Name == "Dizi")
                                    {
                                        <a href="~/AdminBlog/" class="btn btn-warning">Detay</a>
                                    }
                                    else if (kategori.Name == "Film")
                                    {
                                        <a href="~/AdminBlog/MovieList" class="btn btn-warning">Detay</a>
                                    }
                                    else
                                    {
                                        <a href="~/AdminBlog/Book" class="btn btn-warning">Detay</a>
                                    }

                                </div>
                            </div>
                        </div>


                        //~/ Home / CategoryDetail / @kategori.Id
                    }

                    @*<a href="~/AdminBlog/" class="btn btn-warning">Detay2</a>*@

                </div>
            </div>
        </div>
  
    </div>


    <!--pagination-->
    <div class="pagination text-center">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="pagination-area text-center">
                        <div class="pagination-list">
                            <ul class="list-inline">
                                <li><a href="#"><i class="las la-arrow-left"></i></a></li>
                                <li><a href="#" class="active">1</a></li>
                                <li><a href="#">2</a></li>
                                <li><a href="#">3</a></li>
                                <li><a href="#">4</a></li>
                                <li><a href="#"><i class="las la-arrow-right"></i></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--footer-->
    <div class="footer">
        <div class="footer-area">
            <div class="footer-area-content">
                <div class="container-fluid">
                    <div class="row ">
                        <div class="col-md-3">
                            <div class="menu">
                                <h6>Menu</h6>
                                <ul>
                                    <li><a href="#">Homepage</a></li>
                                    <li><a href="#">about us</a></li>
                                    <li><a href="#">contact us</a></li>
                                    <li><a href="#">privarcy</a></li>
                                </ul>
                            </div>
                        </div>
                        <!--newsletter-->
                        <div class="col-md-6">
                            <div class="newslettre">
                                <div class="newslettre-info">
                                    <h3>
                                        Subscribe To Our
                                        Newsletter
                                    </h3>
                                    <p>Sign up for free and be the first to get notified about new posts.</p>

                                </div>
                                <form action="#" class="newslettre-form">
                                    <div class="form-flex">
                                        <div class="form-group">
                                            <input type="email" class="form-control" placeholder="Your Email Adress" required="required">
                                        </div>
                                        <button class="submit-btn" type="submit">
                                            <i class="fas fa-paper-plane"></i>
                                        </button>
                                    </div>
                                </form>
                            </div>
                        </div>
                        <!--/-->
                        <div class="col-md-3">
                            <div class="menu">
                                <h6>Follow us</h6>
                                <ul>
                                    <li><a href="#">facebook</a></li>
                                    <li><a href="#">instagram</a></li>
                                    <li><a href="#">youtube</a></li>
                                    <li><a href="#">twitter</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--footer-copyright-->
            <div class="footer-area-copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="copyright">
                                <p>© 2022,  AssiaGroupe, All Rights Reserved.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--/-->
        </div>
    </div>

    <!--Search-form-->
    <div class="search">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-7 col-md-10 m-auto">
                    <div class="search-width">
                        <button type="button" class="close">
                            <i class="far fa-times"></i>
                        </button>
                        <form class="search-form" action="https://assiagroupe.site/demo/html/template/oredoo/Oredoo/search.html">
                            <input type="search" value="" placeholder="What are you looking for?">
                            <button type="submit" class="search-btn"> search</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="~/Content/Admin/assets/js/jquery.min.js"></script>
